<%- include("../partials/head", {tituloWeb: "Carrito de Compras", cssstyle:
"/css/styles.css"})%>
<body>
  <!--HEADER -->
  <%- include("../partials/header") %>
    <!--CARRO DE COMPRAS-->
    <section class="cart-section">
      <section class="cart-section-item">
        <h2 class="cart-section-item-title">Carro de Compras</h2>
        <div class="cart-container">
          <div class="cart-container-image">
            <img
              class="cart-container-image-img"
              src="/img/mtb-poly-t7-1.jpg"
            />
          </div>
          <div class="cart-container-resume">
            <h2 class="cart-container-product">POLYGON SISKIU T7</h2>
            <h3 class="cart-container-price">
              U$D 2.100,00
              <span class="cart-container-price-discount">20% OFF</span>
            </h3>
            <form action="">
              <div class="cart-container-buy">
                <input
                  class="cart-container-buy-quantity"
                  type="number"
                  id="quantity"
                  name="quantity"
                  min="1"
                  max="3"
                  value="1"
                />
              </div>
            </form>
          </div>
        </div>
      </section>
      <section class="cart-section-summary">
        <h2 class="cart-section-summary-title">Resumen de tu Compra</h2>
      </section>
    </section>
  <!--FOOTER -->
  <%- include("../partials/footer") %>

